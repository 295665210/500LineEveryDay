        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFInRevitDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Botton" Content="点击创建墙" HorizontalAlignment="Left" Margin="622,203,0,0" VerticalAlignment="Top"
                Width="75" Click="Botton_Click" RenderTransformOrigin="0.211,0.058" />
        <Label x:Name="创建墙" Content="输入墙高度" HorizontalAlignment="Left" Margin="78,198,0,0" VerticalAlignment="Top"
               RenderTransformOrigin="-2.65,-0.118" />
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="23" Margin="237,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="358" />

    </Grid>
